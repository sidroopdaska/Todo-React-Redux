{
	"extends": [
		"tslint:latest",
		"tslint-react"
	],
	"rules": {
		"interface-name": false,
		"indent": [
			true,
			"tabs"
		],
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"max-line-length": [
			true,
			160
		],
		"member-ordering": [
			true,
			"static-before-instance",
			"variables-before-functions"
		],
		"jsx-no-multiline-js": false
	}
}